<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- right leg -->
  <xacro:macro name="robotis_op3_r_leg" params="parent">

  <!-- right hip yaw joint -->
  <joint name="r_hip_yaw" type="revolute">
    <parent link="${parent}"/>
    <child link="r_hip_yaw_link"/>
    <origin xyz="0.0 -0.035 0.0" rpy="0 0 0" />
    <axis xyz="0 0 -1" />
    <limit effort="1000" velocity="100" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="r_hip_yaw" /> -->

  <!-- right hip yaw link -->
  <link name="r_hip_yaw_link">
    <visual>
      <xacro:insert_mesh_r_hip_yaw_link />
    </visual>
    <!-- <collision>
      <xacro:insert_mesh_r_hip_yaw_link />
    </collision> -->
    <collision>
      <origin xyz="-0.001500 0.000000 -0.015500" rpy="0 0 0"/>
      <geometry>
        <box size="0.045000 0.025000 0.031000"/>
      </geometry>
    </collision>
    <inertial>
      <xacro:insert_inertia_r_hip_yaw_link />
    </inertial>
  </link>

  <!-- right hip roll joint -->
  <joint name="r_hip_roll" type="revolute">
    <parent link="r_hip_yaw_link"/>
    <child link="r_hip_roll_link"/>
    <origin xyz="-0.024 0.0 -0.0285" rpy="0 0 0" />
    <axis xyz="-1 0 0" />
    <limit effort="1000" velocity="100" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="r_hip_roll" /> -->

  <!-- right hip roll link -->
  <link name="r_hip_roll_link">
    <visual>
      <xacro:insert_mesh_r_hip_roll_link />
    </visual>
    <!-- <collision>
      <xacro:insert_mesh_r_hip_roll_link />
    </collision> -->
    <collision>
      <origin xyz="0.003875 -0.002200 -0.012000" rpy="0 0 0"/>
      <geometry>
        <box size="0.079750 0.042400 0.046500"/>
      </geometry>
    </collision>
    <inertial>
      <xacro:insert_inertia_r_hip_roll_link />
    </inertial>
  </link>

  <!-- right hip pitch joint -->
  <joint name="r_hip_pitch" type="revolute">
    <parent link="r_hip_roll_link"/>
    <child link="r_hip_pitch_link"/>
    <origin xyz="0.0241 -0.019 0.0" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <limit effort="1000" velocity="100" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="r_hip_pitch" /> -->

  <!-- right hip pitch link -->
  <link name="r_hip_pitch_link">
    <visual>
      <xacro:insert_mesh_r_hip_pitch_link />
    </visual>
    <!-- <collision>
      <xacro:insert_mesh_r_hip_pitch_link />
    </collision> -->
    <collision>
      <origin xyz="0.000425 0.018800 -0.059450" rpy="0 0 0"/>
      <geometry>
        <box size="0.030850 0.046400 0.123900"/>
      </geometry>
    </collision>
    <inertial>
      <xacro:insert_inertia_r_hip_pitch_link />
    </inertial>
  </link>

  <!-- right knee joint -->
  <joint name="r_knee" type="revolute">
    <parent link="r_hip_pitch_link"/>
    <child link="r_knee_link"/>
    <origin xyz="0.0 0.0 -0.11015" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <limit effort="1000" velocity="100" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="r_knee" /> -->

  <!-- right hip pitch link -->
  <link name="r_knee_link">
    <visual>
      <xacro:insert_mesh_r_knee_link />
    </visual>
    <!-- <collision>
      <xacro:insert_mesh_r_knee_link />
    </collision> -->
    <collision>
      <origin xyz="0.000000 0.020000 -0.055000" rpy="0 0 0"/>
      <geometry>
        <box size="0.030000 0.044000 0.115000"/>
      </geometry>
    </collision>
    <inertial>
      <xacro:insert_inertia_r_knee_link />
    </inertial>
  </link>

  <!-- right ankle pitch link -->
  <joint name="r_ank_pitch" type="revolute">
    <parent link="r_knee_link"/>
    <child link="r_ank_pitch_link"/>
    <origin xyz="0 0 -0.110" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit effort="1000" velocity="100" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="r_ank_pitch" /> -->

  <!-- right hip pitch link -->
  <link name="r_ank_pitch_link">
    <visual>
      <xacro:insert_mesh_r_ank_pitch_link />
    </visual>
    <!-- <collision>
      <xacro:insert_mesh_r_ank_pitch_link />
    </collision> -->
    <collision>
      <origin xyz="-0.020225 0.016800 0.012000" rpy="0 0 0"/>
      <geometry>
        <box size="0.079750 0.042400 0.046500"/>
      </geometry>
    </collision>
    <inertial>
      <xacro:insert_inertia_r_ank_pitch_link />
    </inertial>
  </link>

  <!-- right ankle roll link -->
  <joint name="r_ank_roll" type="revolute">
    <parent link="r_ank_pitch_link"/>
    <child link="r_ank_roll_link"/>
    <origin xyz="-0.0241 0.019 0" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit effort="1000" velocity="100" lower="${-pi*0.9}" upper="${pi*0.9}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint>
  <!-- <xacro:insert_transmission name="r_ank_roll" /> -->

  <!-- right hip roll link -->
  <link name="r_ank_roll_link">
    <visual>
      <xacro:insert_mesh_r_ank_roll_link />
    </visual>
    <!-- <collision>
      <xacro:insert_mesh_r_ank_roll_link />
    </collision> -->
    <collision>
      <origin xyz="0.024000 -0.012500 -0.0265" rpy="0 0 0"/>
      <geometry>
        <box size="0.127000 0.080000 0.008"/>
      </geometry>
    </collision>
    <inertial>
      <xacro:insert_inertia_r_ank_roll_link />
    </inertial>
  </link>

  </xacro:macro>

</robot>
